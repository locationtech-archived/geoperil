      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>dip</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>34</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>strike</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>164</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
